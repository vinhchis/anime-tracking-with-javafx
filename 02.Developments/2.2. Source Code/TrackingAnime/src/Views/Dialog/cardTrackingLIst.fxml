<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ctl_apTrackingList" prefWidth="200.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.CardTrackingListController">
   <children>
      <Label layoutX="12.0" layoutY="32.0" styleClass="head-label" stylesheets="@../../Assets/css/loginDesgin.css" text="Collections" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0" />
      <VBox layoutX="14.0" layoutY="62.0" prefHeight="162.0" prefWidth="170.0" spacing="10.0">
         <children>
            <Label text="Name">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="ctl_txtTrackingName" />
            <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="142.0" spacing="10.0">
               <children>
                  <Button fx:id="ctl_btnCancel" mnemonicParsing="false" onAction="#canceltrackingbtn" styleClass="btn-cancel" stylesheets="@../../Assets/css/trackinganimeDesign.css" text="Cancel" />
                  <Button fx:id="ctl_btnAdd" mnemonicParsing="false" onAction="#addtrackingbtn" styleClass="btn-schedule" stylesheets="@../../Assets/css/trackinganimeDesign.css" text="Add" />
               </children>
               <padding>
                  <Insets left="5.0" right="5.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
